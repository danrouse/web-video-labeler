{
  "manifest_version": 2,
  "name": "Web Video Labeler",
  "version": "0.0.5",
  "description": "Label videos on the web for training image classifiers",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "bundle.js"
      ],
      "css": [
        "bundle.css"
      ],
      "all_frames": true
    }
  ],
  "permissions": [
    "management",
    "downloads"
  ],
  "background": {
    "scripts": [
      "extension/liveReload.js",
      "extension/downloads.bundle.js"
    ]
  },
  "web_accessible_resources": [
    "prepare_darknet_training_data.py"
  ]
}
