{
  "name": "web-video-labeler",
  "version": "1.0.0",
  "description": "Label videos on the web for training image classifiers",
  "repository": "https://github.com/danrouse/web-video-labeler",
  "main": "dist/main.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "rollup": "rollup -c",
    "tsc": "tsc -p tsconfig.json",
    "copy-static": "cpx \"src/**/*.{css,json}\" dist",
    "crx": "crx pack dist -o dist/web-video-labeler.crx",
    "xpi": "web-ext sign -s dist -a dist -i *.crx -i key.pem",
    "lint": "tslint -p tsconfig.json",
    "build": "npm-run-all --serial clean tsc copy-static rollup",
    "package": "npm-run-all --parallel crx xpi",
    "start": "npm-run-all --parallel -l \"tsc -- -w\" \"copy-static -- -w\" \"rollup -- -w\""
  },
  "author": "Dan Rouse <dan@rouse.house>",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "0.0.73",
    "@types/jszip": "^3.1.4",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "cpx": "^1.5.0",
    "crx": "^3.2.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2",
    "rollup": "^0.65.2",
    "rollup-plugin-alias": "^1.5.1",
    "rollup-plugin-bundle-hash": "^1.0.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "tslint": "^5.12.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.0.3",
    "web-ext": "^2.9.3"
  },
  "dependencies": {
    "jszip": "^3.1.5",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  }
}
